{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-underscore-dangle": [2, { "allow": ["__filename", "__dirname"] }],
        "linebreak-style": 0,
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "js": "always"
            }
        ]
    }
}